@model List<Product>

<h3>Таблиця</h3>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Created Date</th>
    </tr>
    @foreach (var product in Model)
    {
        <tr>
            <td width="50">@product.ID</td>
            <td width="100">@product.Name</td>
            <td width="100">@product.Price</td>
            <td width="150">@product.CreatedDate</td>
        </tr>
    }
</table>

<h3>Список</h3>
<ul>
    @foreach (var product in Model)
    {
        <li>@product.ID. @product.Name – @product.Price$ (@product.CreatedDate)</li>
    }
</ul>